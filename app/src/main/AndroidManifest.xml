<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jjuncoder.sideproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SideProject">

        <activity
            android:name=".compass.CompassActivity"
            android:label="@string/title_activity_compass" />

        <activity
            android:name=".earthquake.view.EarthQuakeActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/Theme.SideProject.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>